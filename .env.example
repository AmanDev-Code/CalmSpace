# Environment Variables for CalmSpace

# Razorpay API Keys
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id

# EmailJS (for contact forms and booking)
VITE_EMAILJS_USER_ID=your_emailjs_user_id
VITE_EMAILJS_SERVICE_ID_CONTACT=your_service_id_contact
VITE_EMAILJS_TEMPLATE_ID_CONTACT=your_template_id_contact
VITE_EMAILJS_SERVICE_ID_BOOKING=your_service_id_booking
VITE_EMAILJS_TEMPLATE_ID_BOOKING=your_template_id_booking
VITE_EMAILJS_OTP_TEMPLATE_ID=your_template_id_otp

# OTP-specific EmailJS configuration
# These are for a separate EmailJS account dedicated to sending OTP emails
VITE_OTP_EMAILJS_USER_ID=your_otp_specific_user_id
VITE_OTP_EMAILJS_SERVICE_ID=your_otp_specific_service_id
VITE_OTP_EMAILJS_TEMPLATE_ID=your_otp_specific_template_id

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket
VITE_FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id
VITE_FIREBASE_APP_ID=your_firebase_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_firebase_measurement_id

# EmailJS Configuration
VITE_EMAILJS_SERVICE_ID=service_xxxxxxxxxx
VITE_EMAILJS_WELCOME_TEMPLATE_ID=template_welcome
VITE_EMAILJS_CONTACT_TEMPLATE_ID=template_contact

# Legacy EmailJS variables (for backward compatibility)
VITE_EMAILJS_SERVICE_ID_CONTACT=service_xxxxxxxxxx
VITE_EMAILJS_TEMPLATE_ID_CONTACT=template_contact
VITE_EMAILJS_SERVICE_ID_BOOKING=service_xxxxxxxxxx
VITE_EMAILJS_TEMPLATE_ID_BOOKING=template_booking 